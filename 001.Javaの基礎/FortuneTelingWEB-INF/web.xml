        <servlet-name>servlet</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FortunTelling</servlet-name>
        <url-pattern>/FortunTelling</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FortuneTelling</servlet-name>
        <url-pattern>/FortuneTelling</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FortuneTeling1</servlet-name>
        <url-pattern>/FortuneTeling1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
